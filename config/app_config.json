{
  "application": {
    "name": "Microsoft Excel",
    "version": "16.0.0",
    "environment": "production",
    "debug_mode": false,
    "max_workbooks_per_user": 100,
    "max_worksheets_per_workbook": 200,
    "auto_save_interval_seconds": 300
  },
  "ui": {
    "theme": "default",
    "language": "en-US",
    "enable_dark_mode": true,
    "default_font": "Calibri",
    "default_font_size": 11
  },
  "performance": {
    "max_threads": 8,
    "cache_size_mb": 512,
    "enable_lazy_loading": true,
    "max_undo_steps": 100
  },
  "features": {
    "enable_real_time_collaboration": true,
    "enable_cloud_storage": true,
    "enable_macros": true,
    "enable_add_ins": true,
    "enable_external_data_connections": true
  },
  "security": {
    "enable_content_encryption": true,
    "password_min_length": 8,
    "password_require_special_char": true,
    "password_require_number": true,
    "max_login_attempts": 5,
    "session_timeout_minutes": 30
  },
  "file_formats": {
    "default_save_format": "xlsx",
    "supported_import_formats": ["xlsx", "xls", "csv", "txt"],
    "supported_export_formats": ["xlsx", "pdf", "csv"]
  },
  "integrations": {
    "onedrive": {
      "enabled": true,
      "api_endpoint": "https://api.onedrive.com/v1.0"
    },
    "sharepoint": {
      "enabled": true,
      "api_endpoint": "https://your-tenant.sharepoint.com/_api"
    },
    "teams": {
      "enabled": true,
      "api_endpoint": "https://api.teams.microsoft.com/v1.0"
    }
  },
  "logging": {
    "level": "info",
    "file_path": "/var/log/excel/app.log",
    "max_file_size_mb": 100,
    "max_files": 5,
    "enable_console_logging": false
  },
  "updates": {
    "check_for_updates_on_startup": true,
    "update_channel": "stable",
    "auto_download_updates": false
  }
}